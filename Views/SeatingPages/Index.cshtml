@model IEnumerable<Seating.Models.SeatingPage>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div style="margin-left:15em; margin-top:2em;" class="col-md-8">
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    Date
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.URL)
                </th>
                <th></th>
            </tr>
    
        @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SeatingDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.URL)
                </td>
                <td>
                    @Html.ActionLink("Delete", "Delete", new { id=item.SeatingId })
                </td>
            </tr>
        }
    
        </table>
    </div>
</body>
</html>
